{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/transform-runtime"
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {},
      "proposal-class-properties"
    ],
    [
      "@babel/plugin-syntax-dynamic-import",
      {},
      "plugin-syntax-dynamic-import"
    ]
  ]
}